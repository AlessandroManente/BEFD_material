# All the libraries
library("readxl")
library(DIMORA)
library(forecast)
library(fpp2)
library(lmtest)
library(sm)
library(KernSmooth)
library(splines)
library(gam)
library (gbm)
data <- read_excel("OS.xlsx")
data
summary(data)
ios = data$iOS
ts_ios = ts(ios, frequency=4)
android = data$Android
ts_android = ts(android, frequency=4)
tsdisplay(ios)
tsdisplay(android)
ios_tslm = tslm(ts_ios~ trend+season)
summary(ios_tslm)
fit_ios_tslm<- fitted(ios_tslm)
plot(ts_ios, col=3)
lines(fit_ios_tslm, col=2)
res_ios_tslm<- residuals(ios_tslm)
plot(res_ios_tslm)
Acf(res_ios_tslm)
checkresiduals(res_ios_tslm)
ios_tslm_a = tslm(ts_ios~ trend+season+ts_android)
summary(ios_tslm_a)
fit_ios_tslm_a<- fitted(ios_tslm_a)
plot(ts_ios, col=3)
lines(fit_ios_tslm_a, col=2)
res_ios_tslm_a<- residuals(ios_tslm_a)
plot(res_ios_tslm_a)
Acf(res_ios_tslm_a)
checkresiduals(res_ios_tslm_a)
ios_BMs<-BASS.standard(ios, display = T)
#Cumulative forecasting
fit_ios_BMs = fitted(ios_BMs)
#Instantaneous forecasting
make.instantaneous(fit_ios_BMs)
#Bass Model estimates (single par.: BMs$Estimate[1,1])
ios_BMs$Estimate
#Cumulative residuals
residuals(ios_BMs)
#Instantaneous residuals
make.instantaneous(residuals(ios_BMs))
#prelimestimates: use the estimations of BM
m=ios_BMs$Estimate[1,1]
p=ios_BMs$Estimate[2,1]
q=ios_BMs$Estimate[3,1]
GBM_ios<-BASS.generalized(ios, shock = "exp", nshock = 1, prelimestimates = c(m, p , q, 17, -0.1, 0.1))#, display=F)
fitted(GBM_ios)
make.instantaneous(fitted(GBM_ios))
arima_ios<- Arima(ios, order = c(0,1,0), seasonal = list(order=c(1,1,1), period=4))
arima_ios
plot(fitted(arima_ios), col=2)
lines(ios, col=3)
res_arima_ios<- residuals(arima_ios)
tsdisplay(res_arima_ios)
checkresiduals(res_arima_ios)
gam_ios <- gam(ios~s(android),arg=c("df=2","df=3","df=4"))
plot(ios, col=3, type="l")
lines(fitted(gam_ios), col=2)
tsdisplay(residuals(gam_ios))
gam_ios <- gam(ios~s(android),arg=c("df=2","df=3","df=4"))
plot(ios, col=3, type="l")
lines(fitted(gam_ios), col=2)
tsdisplay(residuals(gam_ios))
